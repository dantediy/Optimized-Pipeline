[rke-hosts]
gitlab-runner ansible_host="IP"

[rke-hosts:vars]
ansible_user=gitlab-runner
ansible_password="User Pass"
ansible_become_user="Root ID"
ansible_become_password="Root Pass"

[vms]
master ansible_host="IP"
worker1 ansible_host="IP"
worker2 ansible_host="IP"

[vms:vars]
ansible_user="User ID"
ansible_password="User Pass"
ansible_become_password="Root Pass"

